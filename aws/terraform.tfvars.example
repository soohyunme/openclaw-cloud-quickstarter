# === AWS Free Tier (Default) ===
# aws_region    = "us-east-1"
# instance_type = "t3.micro"
# disk_size_gb  = 30
# disk_type     = "gp3"          # gp3 is newer and often slightly better

# === Performance Config (Paid) ===
# Uncomment below for better performance (Cost: ~$10-20/mo)
# instance_type = "t3.small"     # 2 vCPU, 2GB RAM
# disk_size_gb  = 30             # 30GB (Still free tier eligible!)
# disk_type     = "gp3"          # Faster SSD

# === OpenClaw AI Model ===
# openclaw_model = "claude-3-5-sonnet-20241022"
# llm_api_key    = "sk-ant-..."